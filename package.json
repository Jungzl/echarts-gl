{
  "name": "@jungzl/echarts-gl",
  "description": "Extension pack of ECharts providing 3D plots and globe visualization",
  "version": "0.0.1",
  "maintainers": [
    {
      "name": "Jungzl"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jungzl/echarts-gl.git"
  },
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js && webpack --mode development --devtool source-map && webpack --mode production",
    "dev": "webpack --mode development -w --devtool eval-cheap-module-source-map",
    "prepublish": "node scripts/build.js && babel src -d lib",
    "release": "npm run prepublish && npm run build"
  },
  "main": "dist/echarts-gl.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/echarts-gl.cjs"
    }
  },
  "dependencies": {
    "claygl": "^1.2.1",
    "zrender": "^5.1.1"
  },
  "peerDependencies": {
    "echarts": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "benchmark": "^2.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "echarts": "^5.1.2",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.1",
    "http-server": "^0.10.0",
    "maptalks": "^0.38.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "sideEffects": [
    "lib/chart/*.js",
    "lib/component/*.js",
    "lib/echarts-gl.js",
    "lib/export/all.js",
    "src/chart/*.js",
    "src/component/*.js",
    "src/echarts-gl.js",
    "src/export/all.js",
    "index.js"
  ]
}
